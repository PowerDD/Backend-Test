extends layout

block content
	input#msg-items(type="hidden", value="#{__('item(s)')}")
	input#msg-warranty(type="hidden", value="#{__('warranty')}")
	input#msg-year(type="hidden", value="#{__('year')}")
	input#msg-month(type="hidden", value="#{__('month')}")
	input#msg-day(type="hidden", value="#{__('day')}")
	input#msg-itemOnCart(type="hidden", value="#{__('itemOnCart')}")
	input#msg-onOrder(type="hidden", value="#{__('onOrder')}")
	input#msg-orderNow(type="hidden", value="#{__('orderNow')}")
	input#msg-orMoreItems(type="hidden", value="#{__('or more items')}")
	input#msg-ofTheSameCategory(type="hidden", value="#{__('of the same category')}")
	input#msg-ofTheDiffernceCategory(type="hidden", value="#{__('of the differnce category')}")
	input#msg-retailPrice(type="hidden", value="#{__('retailPrice')}")
	input#msg-wholesalePrice(type="hidden", value="#{__('wholesalePrice')}")
	input#msg-price(type="hidden", value="#{__('price')}")
	input#msg-remain(type="hidden", value="#{__('remain')}")
	input#msg-outOfStock(type="hidden", value="#{__('out of stock')}")
	
	#dv-loading.callout.callout-info.text-center
		span.loading.text-muted &nbsp;#{__('loadingData')}
		.overlay
		i.fa.fa-refresh.fa-spin

	