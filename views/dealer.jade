extends layout

block content
	.row
		.col-xs-12
			.box
				.box-header
					h3.box-title Data Table With Full Features
				.box-body
					table#example1.table.table-bordered.table-striped
						thead
							tr
								th วันที่สมัคร
								th ชื่อ-สกุล
								th จังหวัด
								th 
						tbody
							each values in data.dealerRegisterInfo
								- var yearTH = parseInt(data.Moment(values.RegisterDate).lang('th').add(3600*7, 'seconds').format('YYYY'))+543;
							
								tr
									td #{data.Moment(values.RegisterDate).lang('th').add(3600*7, 'seconds').format('DD MMMM')+' '+yearTH}
									td #{values.Firstname+' '+values.Lastname}
									td #{values.Province}
									td X
						 